<?xml version="1.0"?>
<launch>
  
  <group ns="G29">
    <node pkg="joy" type="joy_node" name="joy">
      <param name="coalesce_interval" type="double" value="0.02"/>
      <param name="default_trig_val" value="true"/>
      <param name="dev" value="/dev/input/js0" type="string "/>
      <param name="deadzone" value="0.05" type="double"/>    
    </node>

    <node name="passive" pkg="passive" type="passive_wave.py" output="screen">
    </node>
     
    <node pkg="passive"
                type="RTSP_camera_streaming.sh" name="run_script"
                output="screen"/>
    
 
  </group>
</launch>
